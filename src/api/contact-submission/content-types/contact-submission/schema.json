{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "string"
    },
    "message": {
      "type": "text",
      "maxLength": 5000,
      "required": true
    },
    "status_contact": {
      "type": "enumeration",
      "enum": [
        "new",
        "in progress",
        "resolved"
      ]
    }
  }
}
